
#include <stdio.h>
int main() {
    int num;
    // Ask user to enter a number
    printf("Enter a number: ");
    scanf("%d", &num);
    // Check if number is even or odd
    if (num % 2 == 0) {
        printf("%d is Even", num);
    } else {
        printf("%d is Odd", num);
    }
    return 0;
}
